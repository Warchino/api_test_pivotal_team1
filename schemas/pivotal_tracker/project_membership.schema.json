{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "array",
  "title": "The Root Schema",
  "items": {
    "$id": "#/items",
    "type": "object",
    "title": "The Items Schema",
    "required": [
      "kind",
      "project_id",
      "id",
      "created_at",
      "updated_at",
      "role",
      "project_color",
      "favorite",
      "wants_comment_notification_emails",
      "will_receive_mention_notifications_or_emails",
      "person"
    ],
    "properties": {
      "kind": {
        "$id": "#/items/properties/kind",
        "type": "string",
        "title": "The Kind Schema",
        "default": "",
        "examples": [
          "project_membership"
        ],
        "pattern": "^(.*)$"
      },
      "project_id": {
        "$id": "#/items/properties/project_id",
        "type": "integer",
        "title": "The Project_id Schema",
        "default": 0,
        "examples": [
          2252185
        ]
      },
      "id": {
        "$id": "#/items/properties/id",
        "type": "integer",
        "title": "The Id Schema",
        "default": 0,
        "examples": [
          10024667
        ]
      },
      "last_viewed_at": {
        "$id": "#/items/properties/last_viewed_at",
        "type": "string",
        "title": "The Last_viewed_at Schema",
        "default": "",
        "examples": [
          "2019-02-07T04:31:43Z"
        ],
        "pattern": "^(.*)$"
      },
      "created_at": {
        "$id": "#/items/properties/created_at",
        "type": "string",
        "title": "The Created_at Schema",
        "default": "",
        "examples": [
          "2019-02-07T04:31:43Z"
        ],
        "pattern": "^(.*)$"
      },
      "updated_at": {
        "$id": "#/items/properties/updated_at",
        "type": "string",
        "title": "The Updated_at Schema",
        "default": "",
        "examples": [
          "2019-02-07T04:31:43Z"
        ],
        "pattern": "^(.*)$"
      },
      "role": {
        "$id": "#/items/properties/role",
        "type": "string",
        "title": "The Role Schema",
        "default": "",
        "examples": [
          "owner"
        ],
        "pattern": "^(.*)$"
      },
      "project_color": {
        "$id": "#/items/properties/project_color",
        "type": "string",
        "title": "The Project_color Schema",
        "default": "",
        "examples": [
          "ce9f00"
        ],
        "pattern": "^(.*)$"
      },
      "favorite": {
        "$id": "#/items/properties/favorite",
        "type": "boolean",
        "title": "The Favorite Schema",
        "default": false,
        "examples": [
          false
        ]
      },
      "wants_comment_notification_emails": {
        "$id": "#/items/properties/wants_comment_notification_emails",
        "type": "boolean",
        "title": "The Wants_comment_notification_emails Schema",
        "default": false,
        "examples": [
          true
        ]
      },
      "will_receive_mention_notifications_or_emails": {
        "$id": "#/items/properties/will_receive_mention_notifications_or_emails",
        "type": "boolean",
        "title": "The Will_receive_mention_notifications_or_emails Schema",
        "default": false,
        "examples": [
          true
        ]
      },
      "person": {
        "$id": "#/items/properties/person",
        "type": "object",
        "title": "The Person Schema",
        "required": [
          "kind",
          "id",
          "name",
          "email",
          "initials",
          "username"
        ],
        "properties": {
          "kind": {
            "$id": "#/items/properties/person/properties/kind",
            "type": "string",
            "title": "The Kind Schema",
            "default": "",
            "examples": [
              "person"
            ],
            "pattern": "^(.*)$"
          },
          "id": {
            "$id": "#/items/properties/person/properties/id",
            "type": "integer",
            "title": "The Id Schema",
            "default": 0,
            "examples": [
              3143380
            ]
          },
          "name": {
            "$id": "#/items/properties/person/properties/name",
            "type": "string",
            "title": "The Name Schema",
            "default": "",
            "examples": [
              "abelmallcu123"
            ],
            "pattern": "^(.*)$"
          },
          "email": {
            "$id": "#/items/properties/person/properties/email",
            "type": "string",
            "title": "The Email Schema",
            "default": "",
            "examples": [
              "abelmallcu123@gmail.com"
            ],
            "pattern": "^(.*)$"
          },
          "initials": {
            "$id": "#/items/properties/person/properties/initials",
            "type": "string",
            "title": "The Initials Schema",
            "default": "",
            "examples": [
              "AB"
            ],
            "pattern": "^(.*)$"
          },
          "username": {
            "$id": "#/items/properties/person/properties/username",
            "type": "string",
            "title": "The Username Schema",
            "default": "",
            "examples": [
              "abelmallcu123"
            ],
            "pattern": "^(.*)$"
          }
        }
      }
    }
  }
}