{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "title": "The Root Schema",
  "items": {
    "$id": "#/items",
    "type": "object",
    "title": "The Items Schema",
    "required": [
      "kind",
      "import_api_url",
      "id",
      "project_id",
      "can_import",
      "base_url",
      "is_other",
      "story_name",
      "name",
      "active",
      "created_at",
      "updated_at"
    ],
    "properties": {
      "kind": {
        "$id": "#/items/properties/kind",
        "type": "string",
        "title": "The Kind Schema",
        "default": "",
        "examples": [
          "other_integration"
        ],
        "pattern": "^(.*)$"
      },
      "import_api_url": {
        "$id": "#/items/properties/import_api_url",
        "type": "string",
        "title": "The Import_api_url Schema",
        "default": "",
        "examples": [
          "http://localhost:3000/starwars.xml"
        ],
        "pattern": "^(.*)$"
      },
      "id": {
        "$id": "#/items/properties/id",
        "type": "integer",
        "title": "The Id Schema",
        "default": 0,
        "examples": [
          30
        ]
      },
      "project_id": {
        "$id": "#/items/properties/project_id",
        "type": "integer",
        "title": "The Project_id Schema",
        "default": 0,
        "examples": [
          99
        ]
      },
      "can_import": {
        "$id": "#/items/properties/can_import",
        "type": "boolean",
        "title": "The Can_import Schema",
        "default": false,
        "examples": [
          true
        ]
      },
      "base_url": {
        "$id": "#/items/properties/base_url",
        "type": "string",
        "title": "The Base_url Schema",
        "default": "",
        "examples": [
          "http://localhost:3000"
        ],
        "pattern": "^(.*)$"
      },
      "is_other": {
        "$id": "#/items/properties/is_other",
        "type": "boolean",
        "title": "The Is_other Schema",
        "default": false,
        "examples": [
          true
        ]
      },
      "story_name": {
        "$id": "#/items/properties/story_name",
        "type": "string",
        "title": "The Story_name Schema",
        "default": "",
        "examples": [
          "item"
        ],
        "pattern": "^(.*)$"
      },
      "name": {
        "$id": "#/items/properties/name",
        "type": "string",
        "title": "The Name Schema",
        "default": "",
        "examples": [
          "Sounder Flats Computer Complex"
        ],
        "pattern": "^(.*)$"
      },
      "active": {
        "$id": "#/items/properties/active",
        "type": "boolean",
        "title": "The Active Schema",
        "default": false,
        "examples": [
          true
        ]
      },
      "created_at": {
        "$id": "#/items/properties/created_at",
        "type": "string",
        "title": "The Created_at Schema",
        "default": "",
        "examples": [
          "2019-01-29T12:00:00Z"
        ],
        "pattern": "^(.*)$"
      },
      "updated_at": {
        "$id": "#/items/properties/updated_at",
        "type": "string",
        "title": "The Updated_at Schema",
        "default": "",
        "examples": [
          "2019-01-29T12:00:00Z"
        ],
        "pattern": "^(.*)$"
      }
    }
  }
}
