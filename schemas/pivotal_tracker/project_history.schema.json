{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Project History Schema",
  "required": [
    "header",
    "kind"
  ],
  "properties": {
    "header": {
      "$id": "#/properties/header",
      "type": "array",
      "title": "The Header Schema",
      "items": {
        "$id": "#/properties/header/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "date",
          "points_accepted",
          "points_delivered",
          "points_finished",
          "points_started",
          "points_rejected",
          "points_planned",
          "points_unstarted",
          "points_unscheduled",
          "counts_accepted",
          "counts_delivered",
          "counts_finished",
          "counts_started",
          "counts_rejected",
          "counts_planned",
          "counts_unstarted",
          "counts_unscheduled"
        ],
        "pattern": "^(.+?)$"
      }
    },
    "kind": {
      "$id": "#/properties/kind",
      "type": "string",
      "title": "The Kind Schema",
      "default": "",
      "examples": [
        "daily_history_container"
      ],
      "pattern": "^(.+?)$"
    }
  }
}