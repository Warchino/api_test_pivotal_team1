{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "kind",
    "id",
    "person",
    "account_id",
    "created_at",
    "updated_at",
    "owner",
    "admin",
    "project_creator",
    "timekeeper",
    "time_enterer"
  ],
  "properties": {
    "kind": {
      "$id": "#/properties/kind",
      "type": "string",
      "title": "The Kind Schema",
      "default": "",
      "examples": [
        "account_membership"
      ],
      "pattern": "^(.*)$"
    },
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The Id Schema",
      "default": 0,
      "examples": [
        3143926
      ]
    },
    "person": {
      "$id": "#/properties/person",
      "type": "object",
      "title": "The Person Schema",
      "required": [
        "kind",
        "id",
        "name",
        "email",
        "initials",
        "username"
      ],
      "properties": {
        "kind": {
          "$id": "#/properties/person/properties/kind",
          "type": "string",
          "title": "The Kind Schema",
          "default": "",
          "examples": [
            "person"
          ],
          "pattern": "^(.*)$"
        },
        "id": {
          "$id": "#/properties/person/properties/id",
          "type": "integer",
          "title": "The Id Schema",
          "default": 0,
          "examples": [
            3143926
          ]
        },
        "name": {
          "$id": "#/properties/person/properties/name",
          "type": "string",
          "title": "The Name Schema",
          "default": "",
          "examples": [
            "NeatImp"
          ],
          "pattern": "^(.*)$"
        },
        "email": {
          "$id": "#/properties/person/properties/email",
          "type": "string",
          "title": "The Email Schema",
          "default": "",
          "examples": [
            "NeatImp@mailinator.com"
          ],
          "pattern": "^(.*)$"
        },
        "initials": {
          "$id": "#/properties/person/properties/initials",
          "type": "string",
          "title": "The Initials Schema",
          "default": "",
          "examples": [
            "NE"
          ],
          "pattern": "^(.*)$"
        },
        "username": {
          "$id": "#/properties/person/properties/username",
          "type": "string",
          "title": "The Username Schema",
          "default": "",
          "examples": [
            "neatimp"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "account_id": {
      "$id": "#/properties/account_id",
      "type": "integer",
      "title": "The Account_id Schema",
      "default": 0,
      "examples": [
        1081146
      ]
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The Created_at Schema",
      "default": "",
      "examples": [
        "2019-02-06T15:46:46Z"
      ],
      "pattern": "^(.*)$"
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The Updated_at Schema",
      "default": "",
      "examples": [
        "2019-02-06T15:46:46Z"
      ],
      "pattern": "^(.*)$"
    },
    "owner": {
      "$id": "#/properties/owner",
      "type": "boolean",
      "title": "The Owner Schema",
      "default": false,
      "examples": [
        false
      ]
    },
    "admin": {
      "$id": "#/properties/admin",
      "type": "boolean",
      "title": "The Admin Schema",
      "default": false,
      "examples": [
        false
      ]
    },
    "project_creator": {
      "$id": "#/properties/project_creator",
      "type": "boolean",
      "title": "The Project_creator Schema",
      "default": false,
      "examples": [
        false
      ]
    },
    "timekeeper": {
      "$id": "#/properties/timekeeper",
      "type": "boolean",
      "title": "The Timekeeper Schema",
      "default": false,
      "examples": [
        false
      ]
    },
    "time_enterer": {
      "$id": "#/properties/time_enterer",
      "type": "boolean",
      "title": "The Time_enterer Schema",
      "default": false,
      "examples": [
        false
      ]
    }
  }
}