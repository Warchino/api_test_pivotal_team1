{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "complete",
    "created_at",
    "description",
    "id",
    "kind",
    "position",
    "story_id",
    "updated_at"
  ],
  "properties": {
    "complete": {
      "$id": "#/properties/complete",
      "type": "boolean",
      "title": "The Complete Schema",
      "default": false,
      "examples": [
        false
      ]
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The Created_at Schema",
      "default": "",
      "examples": [
        "2019-02-05T12:00:00Z"
      ],
      "pattern": "^(\\d+-\\d+-\\d+T\\d+:\\d+:\\d+Z)$"
    },
    "description": {
      "$id": "#/properties/description",
      "type": "string",
      "title": "The Description Schema",
      "default": "",
      "examples": [
        "Port 0"
      ],
      "pattern": "^(.*)$"
    },
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The Id Schema",
      "default": 0,
      "examples": [
        5
      ]
    },
    "kind": {
      "$id": "#/properties/kind",
      "type": "string",
      "title": "The Kind Schema",
      "default": "",
      "examples": [
        "task"
      ],
      "pattern": "^(task)$"
    },
    "position": {
      "$id": "#/properties/position",
      "type": "integer",
      "title": "The Position Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "story_id": {
      "$id": "#/properties/story_id",
      "type": "integer",
      "title": "The Story_id Schema",
      "default": 0,
      "examples": [
        559
      ]
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The Updated_at Schema",
      "default": "",
      "examples": [
        "2019-02-05T12:00:00Z"
      ],
      "pattern": "^(\\d+-\\d+-\\d+T\\d+:\\d+:\\d+Z)$"
    }
  }
}