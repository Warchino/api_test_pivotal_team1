{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "The Root Schema",
  "properties": {
    "label": {
      "type": "object",
      "title": "The Label Schema",
      "required": [
        "id",
        "project_id",
        "kind",
        "name",
        "created_at",
        "updated_at"
      ],
      "properties": {
        "id": {
          "$id": "#/properties/label/properties/id",
          "type": "integer",
          "title": "The Id Schema",
          "default": 0,
          "examples": [
            20982654
          ]
        },
        "project_id": {
          "$id": "#/properties/label/properties/project_id",
          "type": "integer",
          "title": "The Project_id Schema",
          "default": 0,
          "examples": [
            2263451
          ]
        },
        "kind": {
          "$id": "#/properties/label/properties/kind",
          "type": "string",
          "title": "The Kind Schema",
          "default": "",
          "examples": [
            "label"
          ],
          "pattern": "^(label)$"
        },
        "name": {
          "$id": "#/properties/label/properties/name",
          "type": "string",
          "minLength": 1,
          "maxLength": 255,
          "title": "The Name Schema",
          "default": "",
          "examples": [
            "utwrxo"
          ],
          "pattern": "^(.*)$"
        },
        "created_at": {
          "$id": "#/properties/label/properties/created_at",
          "type": "string",
          "title": "The Created_at Schema",
          "default": "",
          "examples": [
            "2019-02-08T15:50:52Z"
          ],
          "pattern": "^(\\d+-\\d+-\\d+T\\d+:\\d+:\\d+Z)$"
        },
        "updated_at": {
          "$id": "#/properties/label/properties/updated_at",
          "type": "string",
          "title": "The Updated_at Schema",
          "default": "",
          "examples": [
            "2019-02-08T15:50:52Z"
          ],
          "pattern": "^(\\d+-\\d+-\\d+T\\d+:\\d+:\\d+Z)$"
        }
      }
    }
  }
}